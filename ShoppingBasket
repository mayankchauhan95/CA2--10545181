import json
import requests

class Basket:
    def __init__(self):
        self.__value={}




class Product:
    
    def ap(self,sku,description,seller,price,currency):
        self.sku = sku
        self.description = description
        self.seller = seller
        self.price = price
        self.currency = currency

    def _repr_(self):
        return self._str_()
        
        
        
